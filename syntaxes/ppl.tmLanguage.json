{
  "scopeName": "source.ppl",
  "patterns": [
    {
      "name": "keyword.control.ppl",
      "match": "\\b(?:RAT|VOL|DIR|FIL|INC|DEC|PHN|FUN|STP|PAS|LOP|LPS|LPE|JMP|IF|PRI|PRL|EVN|EVS|EVR|TRG|OUT|BEP|CLD|RUN|REV|STK|SAF)\\b"
    },
    {
      "name": "constant.language.units.ppl",
      "match": "\\b(?:UM|MM|UH|MH|ML|UL)\\b"
    },
    {
      "name": "constant.numeric.ppl",
      "match": "\\b\\d+(?:\\.\\d+)?\\b"
    },
    {
      "name": "entity.name.label.ppl",
      "match": "^:\\w+"
    },
    {
      "name": "comment.line.semicolon.ppl",
      "begin": ";",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.comment.ppl" }
      },
      "end": "$\\n?"
    }
  ],
  "repository": {},
  "injections": {}
}
